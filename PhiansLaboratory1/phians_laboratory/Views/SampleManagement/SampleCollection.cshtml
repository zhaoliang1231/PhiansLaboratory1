@{
    ViewBag.Title = "SampleCollection";
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=10" /> 
    <title></title>
    @using Combres.Mvc
    @Html.CombresLink("siteCss")
    @Html.CombresLink("SampleCollectionJs")
</head>
<body>
    <!--样品领取列表-->
    <div id="sample_collection_datagrid"></div>

    <div style="display:none">
        <div id="sample_collection_toolbar">
            <label>MTR No.</label>
            <input id="MTR_value" style="width: 150px" class="easyui-textbox" />
            <a href="javascript:;" id="search" class="easyui-linkbutton" data-options="iconCls:'icon-search',plain:true">Search</a>
            <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
            @*<a href="#" id="read_sample" class="easyui-linkbutton" data-options="iconCls:'icon-edit',plain:true">Edit Sample</a>
            <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>*@
            <a href="javascript:;" id="Motor" class="easyui-linkbutton" data-options="iconCls:'icon-read',plain:true">Motor List</a>
            <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
            @*<a href="#" id="Take_photo" class="easyui-linkbutton" data-options="iconCls:'icon-photo',plain:true">Take Photo</a>
            <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>*@
            <label style="margin-left:10px">Sended Person：</label>
            <input id="receive_personnel" class="easyui-combobox" style="width: 120px" />
            <a href="javascript:;" id="receive" class="easyui-linkbutton" data-options="iconCls:'icon-ok',plain:true">Receive</a>
            @*<span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
            <a href="#" id="print" class="easyui-linkbutton" data-options="iconCls:'icon-print',plain:true">Print bar code</a>*@
        </div>

        @*位置*@
        <form method="post" id="photo_dialog">
            <div class="h_d" style="margin-top: 10px;">
                <div class="h_dline" style="height: 23px; margin-top: 20px;float: left;margin-left:20px">
                    <label class="h_d1" style="width: 120px;text-align:right;display:inline-block">Position:</label>
                    <input class="easyui-combobox" name="position" style="width: 160px;" id="position" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px;float: left;margin-left:20px">
                    <label class="h_d1" style="width: 120px;text-align:right;display:inline-block">Treasury:</label>
                    <input class="easyui-combobox" name="treasury" style="width: 160px;" id="treasury" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px;float: left;margin-left:20px">
                    <label class="h_d1" style="width: 120px;text-align:right;display:inline-block">Shelves:</label>
                    <input class="easyui-combobox" name="shelves" style="width: 160px;" id="shelves" />
                </div>
            </div>
        </form>

        <!--查看样品详细信息-->
        <form id="Sample_dialog" method="post">
            <div class="h_d" style="margin-top: 30px; margin-left: 20px">
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">MTR NO:</label>
                    <input class="easyui-textbox" name="MTRNO" style="width: 140px;" id="MTRNO" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Sample Position:</label>
                    <input class="easyui-textbox" name="SamplePosition" style="width: 140px;" id="SamplePosition" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Test Load:</label>
                    <input class="easyui-textbox" name="TestLoad" style="width: 140px;" id="TestLoad" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Follow Person:</label>
                    <input class="easyui-textbox" name="FollowUp_n" style="width: 140px;" id="FollowUp_n" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Sample Qty:</label>
                    <input class="easyui-textbox" name="SampleQty" style="width: 140px;" id="SampleQty" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Sample Name:</label>
                    <input class="easyui-textbox" name="SampleName" style="width: 140px;" id="SampleName" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Sample Details:</label>
                    <input class="easyui-textbox" name="SampleDetails" style="width: 140px;" id="SampleDetails" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Sample Description:</label>
                    <input class="easyui-textbox" name="SampleDescription" style="width: 140px;" id="SampleDescription" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Identification:</label>
                    <input class="easyui-combobox" name="Identification" style="width: 140px;" id="Identification" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Appearance:</label>
                    <input class="easyui-combobox" style="width: 140px;" id="Appearance" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Others Status:</label>
                    <input class="easyui-combobox" style="width: 140px;" id="OthersStatus" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Receiving Date:</label>
                    <input class="easyui-datebox" name="ReceivingDate" style="width: 140px;" id="ReceivingDate" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Reviewer:</label>
                    <input class="easyui-textbox" name="Reviewer_n" style="width: 140px;" id="Reviewer_n" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Receiving Signature:</label>
                    <input class="easyui-textbox" name="ReceivingSignature_n" style="width: 140px;" id="ReceivingSignature_n" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Take Out Date:</label>
                    <input class="easyui-datebox" name="TakeOutDate" style="width: 140px;" id="TakeOutDate" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">TakeOut Signature:</label>
                    <input class="easyui-textbox" name="TakeOutSignature_n" style="width: 140px;" id="TakeOutSignature_n" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Retrieval(Y/N):</label>
                    <input class="easyui-combobox" name="Retrieval" style="width: 140px;" id="Retrieval" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Retriever:</label>
                    <input class="easyui-textbox" name="Retriever_n" style="width: 140px;" id="Retriever_n" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">ContactMethod:</label>
                    <input class="easyui-textbox" name="ContactMethod" style="width: 140px;" id="ContactMethod" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Retrieval Date:</label>
                    <input class="easyui-datebox" name="RetrievalDate" style="width: 140px;" id="RetrievalDate" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Status:</label>
                    <input class="easyui-textbox" name="Status" style="width: 140px;" id="Status" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px; text-align: right; display: inline-block">Remarks:</label>
                    <input id="Remarks" class="easyui-textbox" name="Remarks" style="width: 140px;" readonly="readonly" />
                </div>
            </div>
        </form>
        <form id="Vertify_dialog" method="post">
            <div class="h_dline" style="height: 23px; margin-top: 20px; float: left;">
                <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Vertify Code:</label>
                <input id="Vertify_Code" class="easyui-textbox" name="" style="width: 200px;" />
            </div>
        </form>
        <!--电机信息-->
        <form id="datagrid_dialog" method="post">
            @*电机信息列表*@
            <div id="Motor_datagrid"></div>
            @*电机信息工具栏*@
            <div id="Motor_datagrid_toolbar">
                <a href="javascript:;" id="AddRecord" class="easyui-linkbutton" data-options="iconCls:'icon-edit',plain:true">Information</a>
                <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
                <a href="javascript:;" id="place" class="easyui-linkbutton" data-options="iconCls:'icon-place',plain:true">Place</a>
                <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
                <a href="javascript:;" id="print1" class="easyui-linkbutton" data-options="iconCls:'icon-print',plain:true">Print bar code</a>
            </div>
        </form>
        @*修改电机信息*@
        <form method="post" id="Motor_dialog">
            <div class="h_d" style="margin-top: 30px;">
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">MTR NO:</label>
                    <input class="easyui-textbox" name="MTRNO" style="width: 140px;" id="MTRNO" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Motor Num:</label>
                    <input class="easyui-textbox" name="MotoNum" style="width: 140px;" id="MotoNum" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Sort Num:</label>
                    <input class="easyui-textbox" name="SortNum" style="width: 140px;" id="SortNum" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Scrap Date:</label>
                    <input class="easyui-datebox" name="ScrapDate" style="width: 140px;" id="ScrapDate" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Scrap Remarks:</label>
                    <input class="easyui-textbox" name="ScrapRemarks" style="width: 140px;" id="ScrapRemarks" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Sample Position:</label>
                    <input class="easyui-textbox" name="SamplePosition" style="width: 140px;" id="SamplePosition" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Scrap:</label>
                    <input class="easyui-combobox" style="width: 140px;" id="Scrap" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Remarks:</label>
                    <input class="easyui-textbox" name="Remarks" style="width: 440px;height:66px" id="Remarks" />
                </div>
            </div>

        </form>
        <!--照相信息-->
        <div id="take_photo_dialog">
            <div id="cc" class="easyui-layout" style="width:1000px;height:500px;">
                <div data-options="region:'east'" style="width:450px;">
                    <div id="take_photo_datagrid"></div>
                    @*电机信息工具栏*@
                    <div id="take_photo_datagrid_toolbar">
                        <a href="javascript:;" id="take_photo_View" class="easyui-linkbutton" data-options="iconCls:'icon-search',plain:true">View photo</a>
                        <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
                        <a href="javascript:;" id="take_photo_delete" class="easyui-linkbutton" data-options="iconCls:'icon-remove',plain:true">Delete photo</a>
                    </div>
                </div>
                <div data-options="region:'west'" style="width:530px;">
                    <iframe id="WebCam_photo" src="" width="520" height="450" frameborder="0" scrolling="no"></iframe>
                </div>
            </div>
        </div>
        @*拍照信息列表预览*@
        <div id="take_photo_view_dialog">
            <img id="take_photo_view_look" src="" />
        </div>
    </div>
</body>
</html>

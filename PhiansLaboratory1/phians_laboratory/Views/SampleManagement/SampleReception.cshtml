                        <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Retrieval(Y/N):</label>
                        <input class="easyui-combobox" name="Retrieval" style="width: 140px;" id="Retrieval" readonly="readonly" data-options="editable:false" />
                    </div>
                    <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                        <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Retriever:</label>
                        <input class="easyui-combobox" name="Retriever_n" style="width: 140px;" id="Retriever" readonly="readonly" />
                    </div>
                    <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                        <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">ContactMethod:</label>
                        <input class="easyui-textbox" name="ContactMethod" style="width: 140px;" id="ContactMethod" readonly="readonly" />
                    </div>
                    <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                        <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Retrieval Date:</label>
                        <input class="easyui-datebox" name="RetrievalDate" style="width: 140px;" id="RetrievalDate" readonly="readonly" data-options="editable:false" />
                    </div>*@
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Status:</label>
                    <input class="easyui-combobox" name="Status" style="width: 140px;" id="Status" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Description:</label>
                    <input class="easyui-textbox" name="SampleDescription" style="width: 140px;" readonly="readonly" data-options="multiline:true" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Sample Details:</label>
                    <input class="easyui-textbox" name="SampleDetails" style="width: 140px; height: 60px;" readonly="readonly" data-options="multiline:true" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px; text-align: right; display: inline-block">Remarks:</label>
                    <input class="easyui-textbox" name="Remarks" style="width: 140px;height:60px;" readonly="readonly" data-options="multiline:true" />
                </div>
            </div>
        </form>
        @* 提交表单 *@
        <form method="post" id="submitSampleReceive">
            <div class="h_d" style="margin-top: 20px;">
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px; float: left;">
                    <label class="h_d1" style="width: 130px;text-align:right;display:inline-block">Review Group:</label>
                    <input id="Group" class="easyui-combobox" name="" style="width: 220px;" required="required" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px; float: left;">
                    <label class="h_d1" style="width: 130px; text-align: right; display: inline-block">Reviewer:</label>
                    <input id="ApprovedBy" class="easyui-combobox" name="" style="width: 220px; " required="required" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px; float: left;">
                    <label class="h_d1" style="width: 130px; text-align: right; display: inline-block">Test Load:</label>
                    <span style="width: 220px; display: inline-block; ">
                        <input id="TestLoad_true" type="radio" name="TestLoad" value="true" checked="checked" />Attach Load
                        <input id="TestLoad_false" type="radio" name="TestLoad" value="false" />NO Attach Load
                    </span>
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px; float: left;">
                    <label class="h_d1" style="width: 130px; text-align: right; display: inline-block">Identification:</label>
                    <span style="width: 220px; display: inline-block; ">
                        <input id="Identification_true" type="radio" name="Identification" value="true" checked="checked" />Normal
                        <input id="Identification_false" type="radio" name="Identification" value="false" />Abnormal
                    </span>
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px; float: left;">
                    <label class="h_d1" style="width: 130px; text-align: right; display: inline-block">Appearance:</label>
                    <span style="width: 220px; display: inline-block; ">
                        <input id="Appearance_true" type="radio" name="Appearance" value="true" checked="checked" />Normal
                        <input id="Appearance_false" type="radio" name="Appearance" value="false" />Abnormal
                    </span>
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 20px; margin-left: 10px; float: left;">
                    <label class="h_d1" style="width: 130px; text-align: right; display: inline-block">Others Status:</label>
                    <span style="width: 220px; display: inline-block; ">
                        <input id="OthersStatus_true" type="radio" name="OthersStatus" value="true" checked="checked" />Normal
                        <input id="OthersStatus_false" type="radio" name="OthersStatus" value="false" />Abnormal
                    </span>
                </div>
                <div class="h_dline" style="height: 66px; margin-top: 20px; margin-left: 10px; float: left;">
                    <label class="h_d1" style="width: 130px; text-align: right; display: inline-block">Sample Details:</label>
                    <input class="easyui-textbox" style="width: 500px;height:66px" id="SampleDetails" name="SampleDetails" />
                </div>
                <div class="h_dline" style="height: 66px; margin-top: 20px; margin-left: 10px; float: left;">
                    <label class="h_d1" style="width: 130px; text-align: right; display: inline-block">Sample Description:</label>
                    <input class="easyui-textbox" style="width: 500px;height:66px" id="Description" name="SampleDescription" />
                </div>
            </div>
        </form>
        <!--电机信息-->
        <form id="datagrid_dialog" method="post">
            @*电机信息列表*@
            <div id="Motor_datagrid"></div>
            @*电机信息工具栏*@
            <div id="Motor_datagrid_toolbar">
                @*<a href="javascript:;" id="AddRecord" class="easyui-linkbutton" data-options="iconCls:'icon-edit',plain:true">Information</a>
                    <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>*@
                <a href="javascript:;" id="Position" class="easyui-linkbutton" data-options="iconCls:'icon-place',plain:true">Position</a>
                <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
                @*<a href="#" id="generate_samples" class="easyui-linkbutton" data-options="iconCls:'icon-read',plain:true">Generate samples</a>
                    <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>*@
                @*<a href="#" id="del" class="easyui-linkbutton" data-options="iconCls:'icon-no',plain:true">Invalid</a>
                    <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>*@
                <a href="javascript:;" id="print2" class="easyui-linkbutton" data-options="iconCls:'icon-print',plain:true">Print View QR Code</a>
                <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
                <a href="javascript:;" id="print1" class="easyui-linkbutton" data-options="iconCls:'icon-print',plain:true">Print  Selected</a>
                <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
                <a href="javascript:;" id="print3" class="easyui-linkbutton" data-options="iconCls:'icon-print',plain:true">Print  All</a>
            </div>
        </form>
        @* 生成样品弹框 *@
        <form id="generate_samples_dailog" method="post">
            <div class="h_d" style="margin-top: 30px;margin-left:30px">
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 80px;text-align:right;display:inline-block">Sample Num:</label>
                    <input class="easyui-textbox" name="SampleNum" style="width: 300px;height:50px;" id="SampleNum" required="required" />
                </div>
            </div>
        </form>
        @*修改电机信息*@
        <form method="post" id="Motor_dialog">
            <div class="h_d" style="margin-top: 30px;">
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                    <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">MTR NO:</label>
                    <input class="easyui-textbox" name="MTRNO" style="width: 140px;" id="MTRNO1" readonly="readonly" />
                </div>
                <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">

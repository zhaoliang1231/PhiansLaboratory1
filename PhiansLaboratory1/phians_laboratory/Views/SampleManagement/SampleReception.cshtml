</head>
<body>
    <!--<%-- 扫描条码 --%>-->
    <div id="sample_reception_datagrid"></div>

    <div style="display:none">
        @* 样品接收列表工具栏 *@
        <div id="sample_reception_toolbar">
            <label>MTR No.</label>
            <input id="MTR_value" style="width: 150px" class="easyui-textbox" />
            <a href="javascript:;" id="search" class="easyui-linkbutton" data-options="iconCls:'icon-search',plain:true">Search</a>
            <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
            <a href="javascript:;" id="read_sample" class="easyui-linkbutton" data-options="iconCls:'icon-Document',plain:true">View Sample</a>
            <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
            <a href="javascript:;" id="Motor" class="easyui-linkbutton" data-options="iconCls:'icon-read',plain:true">Motor List</a>
            <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
            <a href="javascript:;" id="Take_photo" class="easyui-linkbutton" data-options="iconCls:'icon-photo',plain:true">Take Photo</a>
            <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
            <a href="javascript:;" id="receive_sample" class="easyui-linkbutton" data-options="iconCls:'icon-ok',plain:true">Receive</a>
            <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
            <a href="javascript:;" id="print" class="easyui-linkbutton" data-options="iconCls:'icon-print',plain:true">Print QR Code All</a>
        </div>
        @*储存位置*@
        <form method="post" id="position_dialog">
            @*位置列表*@
            <div id="position_datagrid"></div>
            @*位置列表工具栏*@
            <div id="position_datagrid_toolbar" style="display:none">
                <label>Area:</label>
                <input id="area" style="width: 150px" class="easyui-combobox" required="required" data-options="editable:false" />
                <span class="datagrid-btn-separator" style="vertical-align: middle; height: 22px; display: inline-block; float: none"></span>
                <label>Number of copies:</label>
                <input id="times" style="width: 150px" class="easyui-numberbox" required="required" readonly="readonly" />
            </div>
        </form>

        <!--查看样品详细信息-->

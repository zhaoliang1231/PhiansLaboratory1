                        <input class="easyui-combobox" name="TestLoad" style="width: 140px;" id="TestLoad" readonly="readonly" />
                    </div>*@
                @*<div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">
                        <label class="h_d1" style="width: 140px;text-align:right;display:inline-block">Identification:</label>
                        <input class="easyui-combobox" name="Identification" style="width: 140px;" id="Identification" readonly="readonly" />
                    </div>
                    <div class="h_dline" style="height: 23px; margin-top: 10px; margin-left: 10px;float: left;">

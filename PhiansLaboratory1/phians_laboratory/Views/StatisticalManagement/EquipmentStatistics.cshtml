@{
    ViewBag.Title = "EquipmentStatistics";
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=10" />
    <title> 设备使用率统计</title>
    @using Combres.Mvc
    @Html.CombresLink("siteCss")
    @Html.CombresLink("EquipmentStatisticsJs")
</head>
<body>
    <div style="height:40px;margin-top:30px; text-align:center;">
        <form>
            <label>年份：</label>
            <input class="easyui-datebox" id="planYear" name="planYear" value="2018" style="width:120px" data-options="required:true,formatter:yearFormatter,parser:yearParser" />
            <span style="display: inline-block;width: 30px"></span>
            @*<a href="javascript:;" class="easyui-linkbutton" id="equipmentType">选择设备类型</a>
                <span style="display: inline-block;width: 30px"></span>*@
            <div id='allCheckbox' style="display: inline;">
                <label>操作类型：</label>
                <label for="JobType1">保养</label><input id="JobType1" name="JobType" type="checkbox" value="1" checked="checked" />
                <label for="JobType2">安装</label><input id="JobType2" name="JobType" type="checkbox" value="2" />
                <label for="JobType3">维修</label><input id="JobType3" name="JobType" type="checkbox" value="3" />
                <label for="JobType4">测试</label><input id="JobType4" name="JobType" type="checkbox" value="4" />
            </div>
            <span style="display: inline-block;width: 30px"></span>

            <label>类型选择：</label>
            <input id="TypeChoose" class="easyui-combobox" style="width: 250px" />
            <span style="display: inline-block;width: 30px"></span>

            <a href="javascript:;" id="search" class="easyui-linkbutton" data-options="iconCls:'icon-search',plain:true">Search</a>
        </form>
    </div>
    <div id="Chart1">
        <div id="container1" style="width:100%;height:600px;"></div>
    </div>
    <div id="Chart2" style="display: none">
        <div id="container2"></div>
    </div>
    <div id="Chart3" style="display: none">
        <div id="container3"></div>
    </div>
    <div id="EquipmentTypeDialog" style="overflow-x:hidden">
        <form id="equipmentTypeForm" method="post"></form>
    </div>
</body>
</html>
<script src="~/Scripts/echarts/echarts.js"></script>